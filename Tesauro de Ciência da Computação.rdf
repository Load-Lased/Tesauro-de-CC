    <owl:NamedIndividual rdf:about="http://lod.unicentro.br/2017/DiretrizesSBC#computacaoNuvem">
        <rdf:type rdf:resource="http://lod.unicentro.br/2017/DiretrizesSBC#Conteudo"/>
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
