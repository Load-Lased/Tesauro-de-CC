    <!-- http://lod.unicentro.br/2017/DiretrizesSBC#CompetenciaDerivada -->

    <owl:Class rdf:about="http://lod.unicentro.br/2017/DiretrizesSBC#CompetenciaDerivada">
        <rdfs:isDefinedBy>http://lod.unicentro.br/2017/DiretrizesSBC</rdfs:isDefinedBy>
        <rdfs:label xml:lang="pt">Competência Derivada</rdfs:label>
        <skos:definition xml:lang="pt">É uma lista de competências e/ou habilidades, as quais, possibilitam o bachalerado em Ciência da Computação a lidar profissionalmente com as várias facetas das atividades de computação.</skos:definition>
    </owl:Class>
    


    <!-- http://lod.unicentro.br/2017/DiretrizesSBC#CompetenciaEixo -->

    <owl:Class rdf:about="http://lod.unicentro.br/2017/DiretrizesSBC#CompetenciaEixo">
        <rdfs:isDefinedBy>http://lod.unicentro.br/2017/DiretrizesSBC</rdfs:isDefinedBy>
        <rdfs:label xml:lang="pt">Competência do Eixo</rdfs:label>
        <skos:definition xml:lang="pt">É uma descrição da competência assosciada ao eixo de formação, trazendo seus objetivos propostos em questão.</skos:definition>
    </owl:Class>
    


    <!-- http://lod.unicentro.br/2017/DiretrizesSBC#CompetenciaEspecifica -->

    <owl:Class rdf:about="http://lod.unicentro.br/2017/DiretrizesSBC#CompetenciaEspecifica">
        <rdfs:isDefinedBy>http://lod.unicentro.br/2017/DiretrizesSBC</rdfs:isDefinedBy>
        <rdfs:label xml:lang="pt">Competência Específica</rdfs:label>
        <skos:definition xml:lang="pt">A competência específica se trata de competências e/ou habilidades específicas dos egressos dos Cursos de Bachalerado de Ciência da Computação</skos:definition>
    </owl:Class>
    


    <!-- http://lod.unicentro.br/2017/DiretrizesSBC#CompetenciaGeral -->

    <owl:Class rdf:about="http://lod.unicentro.br/2017/DiretrizesSBC#CompetenciaGeral">
        <rdfs:isDefinedBy>http://lod.unicentro.br/2017/DiretrizesSBC</rdfs:isDefinedBy>
        <rdfs:label xml:lang="pt">Competência Geral</rdfs:label>
        <skos:definition xml:lang="pt">São competências e/ou habilidades gerais dos egressos dos Cursos de Bacharelado e Licenciatura</skos:definition>
    </owl:Class>
    


    <!-- http://lod.unicentro.br/2017/DiretrizesSBC#Conteudo -->

    <owl:Class rdf:about="http://lod.unicentro.br/2017/DiretrizesSBC#Conteudo">
        <rdfs:isDefinedBy>http://lod.unicentro.br/2017/DiretrizesSBC</rdfs:isDefinedBy>
        <rdfs:label xml:lang="pt">Conteúdo</rdfs:label>
        <skos:definition xml:lang="pt">Trata-se de uma lista de conhecimentos que devem ser trabalhados em disciplinas para desenvolver uma competência derivada. Um conteúdo pode ser abordado por mais de uma disciplina, estar presente em mais de uma competência e em mais de um eixo.</skos:definition>
    </owl:Class>
    


    <!-- http://lod.unicentro.br/2017/DiretrizesSBC#Eixo -->

    <owl:Class rdf:about="http://lod.unicentro.br/2017/DiretrizesSBC#Eixo">
        <rdfs:isDefinedBy>http://lod.unicentro.br/2017/DiretrizesSBC</rdfs:isDefinedBy>
        <rdfs:label xml:lang="pt">Eixo</rdfs:label>
        <skos:definition xml:lang="pt">São diferentes eixos de formação que objetivam capacitar o egresso em determinadas competências derivadas. Cada eixo trabalha relacionando conteúdos que podem ser úteis no desenvolvimento das competências que agrega.</skos:definition>
    </owl:Class>
    


    <!-- http://lod.unicentro.br/2017/DiretrizesSBC#TaxonomiaBLOOM -->

    <owl:Class rdf:about="http://lod.unicentro.br/2017/DiretrizesSBC#TaxonomiaBLOOM">
